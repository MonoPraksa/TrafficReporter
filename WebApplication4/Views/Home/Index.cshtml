@model TrafficReporter.Model.Report

@{
    ViewBag.Title = "Home Page";
}

@using (Html.BeginForm("AddReport", "Home", FormMethod.Post))
{
    @Html.TextBoxFor(r => r.Lattitude)

    @Html.TextBoxFor(r => r.Longitude)

    @Html.TextBoxFor(r => r.Rating)

    <input type="submit" value="Submit"/>
}